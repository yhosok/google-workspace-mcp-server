{
  "name": "google-workspace-mcp-server",
  "version": "1.0.0",
  "description": "MCP server for Google Workspace integration",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node --esm src/index.ts",
    "test": "NODE_ENV=test jest --maxWorkers=75% --no-coverage",
    "test:watch": "NODE_ENV=test jest --watch --maxWorkers=50% --no-coverage",
    "test:coverage": "NODE_ENV=test jest --coverage --maxWorkers=75%",
    "test:ci": "NODE_ENV=test jest --ci --coverage --maxWorkers=2",
    "test:verbose": "NODE_ENV=development jest --verbose --maxWorkers=75%",
    "test:fast": "NODE_ENV=test jest --maxWorkers=100% --cache --no-coverage",
    "test:unit": "NODE_ENV=test jest --testPathIgnorePatterns=integration --maxWorkers=75%",
    "test:integration": "NODE_ENV=test jest --testPathPattern=integration --maxWorkers=50%",
    "test:debug": "NODE_ENV=test jest --maxWorkers=1 --verbose --no-cache",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.0",
    "googleapis": "latest",
    "neverthrow": "^8.2.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/jest": "^29.0.0",
    "@types/node": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "ts-jest": "^29.0.0",
    "ts-node": "^10.0.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
